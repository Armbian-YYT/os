name: Test
on:
  workflow_dispatch:

jobs:
  build:

    name: "Team check"
    runs-on: [ "ubuntu-latest" ]
    steps:
     - uses: sgpublic/delete-release-action@v1.1
       with:
         release-drop: true
         release-keep-count: 2
         release-drop-tag: true
         pre-release-drop: true
         pre-release-keep-count: -1
         pre-release-drop-tag: true
         draft-drop: true
         draft-drop-count: -1
       env:
         GITHUB_TOKEN: ${{ secrets.TOKEN }}
